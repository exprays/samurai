{
  "server": {
    "host": "localhost",
    "port": 8080,
    "environment": "development",
    "read_timeout": 30,
    "write_timeout": 30,
    "enable_metrics": true,
    "enable_profiling": true,
    "cors_origins": ["http://localhost:3000", "http://localhost:8080"]
  },
  "database": {
    "host": "localhost",
    "port": 5432,
    "user": "postgres",
    "password": "postgres",
    "dbname": "samurai_dev",
    "sslmode": "disable",
    "max_open_conns": 25,
    "max_idle_conns": 5,
    "enable_logging": true
  },
  "logger": {
    "level": "debug",
    "format": "console",
    "enable_console": true,
    "enable_file": true
  },
  "auth": {
    "jwt_secret": "dev-jwt-secret-key-change-in-production",
    "token_duration": "24h",
    "password_salt_cost": 10
  },
  "security": {
    "encryption_key": "dev-encryption-key-32-characters",
    "enable_rate_limiting": false,
    "enable_csrf_protection": false
  },
  "secrets": {
    "provider": "local",
    "enable_encryption": true,
    "local": {
      "secret_file": "config/secrets.json"
    }
  },
  "external": {
    "llm_providers": {
      "openai": {
        "provider": "openai",
        "enabled": false,
        "model": "gpt-4",
        "max_tokens": 4096,
        "temperature": 0.7
      }
    },
    "cache_provider": {
      "provider": "redis",
      "address": "localhost:6379",
      "enabled": false
    }
  }
}